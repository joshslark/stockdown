{
  "name": "StockDown",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "coverage": "jest --collectCoverage",
    "watch": "jest --watch",
    "lint": "eslint"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@react-native-community/async-storage": "^1.6.1",
    "babel-preset-react-native": "^4.0.1",
    "expo-sqlite": "^6.0.0",
    "gradle": "^1.0.9",
    "help": "^3.0.2",
    "ios-deploy": "^1.9.4",
    "metro": "^0.56.0",
    "react": "16.9.0",
    "react-devtools-core": "msand/react-devtools-core#4c30959",
    "react-dom": "^16.9.0",
    "react-native": "0.61.1",
    "react-native-camera": "^3.3.2",
    "react-native-elements": "^1.2.0",
    "react-native-geolocation": "^1.0.0",
    "react-native-mock-render": "^0.1.5",
    "react-native-permissions": "^1.2.0",
    "react-native-sqlite-storage": "^4.0.0",
    "react-native-unimodules": "^0.5.4",
    "react-native-vector-icons": "^6.6.0"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "24.9.0",
    "detox": "^14.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.1",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "24.9.0",
    "jest-cli": "^24.5.0",
    "jest-react-native": "^18.0.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.18.2",
    "react-devtools-core": "msand/react-devtools-core#4c30959",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "^16.9.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/e2e/"
    ],
    "setupFiles": [
      "./jest/setup.js"
    ]
  },
  "detox": {
    "test-runner": "jest",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/StockDown-Debug.app",
        "build": "xcodebuild -workspace ios/StockDown.xcworkspace -scheme StockDown-Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Pixel_3_API_28"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Pixel_3_API_28"
      }
    }
  }
}
